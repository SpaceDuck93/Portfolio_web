  			<h1 class="h1_home wow fadeIn" data-wow-delay="0.4s">Alfredo Perez</h1>
  			<h3 class="h3_home wow fadeIn" data-wow-delay="0.6s">Web Developer</h3>
  		</div>
          <!-- /.container -->
  		<div class="col-xs-12 text-center abcen wow fadeIn">
  			<div class="button_down ">
  				<a class="imgcircle wow bounceInUp" data-wow-duration="1.5s"  href="#skills"><%= image_tag("icon/circle.png", class: "icon_scroll") %></a>
  			</div>
  		</div>
    </div>

	<!-- NavBar-->
	<nav class="navbar-default" role="navigation">
		<div class="container">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
			</div>

			<div class="collapse navbar-collapse navbar-right navbar-ex1-collapse">
				<ul class="nav navbar-nav">

					<li class="menuItem"><a href="#skills">Skills</a></li>
					<li class="menuItem"><a href="#portfolio">Portfolio</a></li>
					<li class="menuItem"><a href="#screen">Screenshot</a></li>
					<li class="menuItem"><a href="#education">Education</a></li>
					<li class="menuItem"><a href="#contact">Contact</a></li>
				</ul>
			</div>

		</div>
	</nav>

  <%= render 'skills' %>
  <%= render 'portfolio' %>
  <%= render 'screen' %>
  <%= render 'education' %>

	<!-- Banner Download -->
	<div id="downloadlink" class="banner">
		<div class="container">
			<div class="row">
				<div class="col-md-6 col-md-offset-3 text-center wrap_title">
					<a href='http://www.freepik.com/free-vector/colored-polygonal-banners_813265.htm'></a>
					<h2>-Nelson Mandela</h2>
					<p class="lead quote" style="margin-top:20%;">"Education is the most powerful weapon we can use to change the world."</p>
				</div>
			</div>
		</div>
	</div>

<%= render 'contact' %>

